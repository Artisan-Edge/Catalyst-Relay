{
  "name": "catalyst-relay",
  "version": "0.2.0",
  "description": "TypeScript middleware for SAP ADT integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run src/server.ts",
    "build": "tsup",
    "test": "bun test",
    "test:run": "bun test",
    "test:node": "node --experimental-strip-types -e \"import('./dist/index.js')\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "bun run build && bun run test:node"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.11",
    "diff": "^8.0.2",
    "hono": "^4.6.0",
    "undici": "^7.16.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/diff": "^8.0.0",
    "@types/node": "^22.0.0",
    "bun-types": "^1.3.3",
    "playwright": "^1.49.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "playwright": "^1.40.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "sap",
    "adt",
    "abap",
    "middleware",
    "api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
